   RowBox[{"A", "*", 
    RowBox[{"Exp", "[", 
     RowBox[{"I", "*", "k", "*", "x"}], "]"}]}], "+", 
   RowBox[{"B", "*", 
    RowBox[{"Exp", "[", 
     RowBox[{
      RowBox[{"-", "I"}], "*", "k", "*", "x"}], "]"}]}]}]}]], "Input",
 CellChangeTimes->{{3.5994110968353577`*^9, 3.599411104640053*^9}, {
