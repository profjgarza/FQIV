Cell[BoxData[{
 RowBox[{"Marco", " ", 
  RowBox[{"te\[OAcute]rico", ":", " ", 
   RowBox[{
   "Soluci\[OAcute]n", " ", "de", " ", "la", " ", "ecuaci\[OAcute]n", " ", 
    "de", " ", "Schroedinger", " ", "unidimensional", " ", "cuando", " ", 
    "el", " ", "potencial", " ", "tiene", " ", "la", " ", 
    "forma"}]}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"V", 
   RowBox[{"(", "x", ")"}]}], "=", 
  RowBox[{"\[Piecewise]", GridBox[{
     {"\[Infinity]", 
      RowBox[{"x", "\[LessEqual]", "0"}]},
     {"0", 
      RowBox[{"0", "<", "x", "<", "L"}]},
     {"\[Infinity]", 
      RowBox[{"x", "\[GreaterEqual]", "L"}]}
    },
    GridBoxAlignment->{
     "Columns" -> {{Left}}, "ColumnsIndexed" -> {}, "Rows" -> {{Baseline}}, 
      "RowsIndexed" -> {}},
    GridBoxItemSize->{
     "Columns" -> {{Automatic}}, "ColumnsIndexed" -> {}, "Rows" -> {{1.}}, 
      "RowsIndexed" -> {}},
    GridBoxSpacings->{"Columns" -> {
        Offset[0.27999999999999997`], {
         Offset[0.84]}, 
        Offset[0.27999999999999997`]}, "ColumnsIndexed" -> {}, "Rows" -> {
        Offset[0.2], {
         Offset[0.4]}, 
        Offset[0.2]}, "RowsIndexed" -> {}}]}]}], "\[IndentingNewLine]", 
 RowBox[{"Para", " ", "este", " ", "caso", " ", "las", " ", "soluciones", " ",
   "tienen", " ", "la", " ", "forma"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    SubscriptBox["\[Psi]", "n"], 
    RowBox[{"(", "x", ")"}]}], "=", 
   RowBox[{
    RowBox[{
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"2", "/", "L"}], ")"}], 
      RowBox[{"1", "/", "2"}]], "sen", 
     RowBox[{"(", 
      RowBox[{"n\[Pi]x", "/", "L"}], ")"}], " ", "con", " ", "n"}], "=", 
    "1"}]}], ",", "2", ",", "..", ",", "\[Infinity]"}]}], "Text",
